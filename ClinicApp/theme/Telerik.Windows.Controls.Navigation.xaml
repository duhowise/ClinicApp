                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="-90"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Grid.RenderTransform>
                </Grid>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="Arrow" Value="{telerik:Office2013Resource ResourceKey=EffectAccentLowBrush}"/>
                <Setter Property="BorderBrush" TargetName="Arrow" Value="{telerik:Office2013Resource ResourceKey=AccentMainBrush}"/>
