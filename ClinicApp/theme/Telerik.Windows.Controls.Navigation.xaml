                                        <DoubleAnimation Storyboard.TargetName="HeaderElement" Storyboard.TargetProperty="Opacity" Duration="0" To="0.3"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}"/>
                        <Border x:Name="MouseOverVisual" Opacity="0" Background="{telerik:Office2013Resource ResourceKey=EffectAccentLowBrush}"/>
                        <Border x:Name="SelectedVisual" Opacity="0" Background="{telerik:Office2013Resource ResourceKey=EffectAccentHighBrush}"/>
                        <Grid Margin="{TemplateBinding Padding}">
