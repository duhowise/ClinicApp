                    <Path
                            Data="M4,0L4,7 0,3.35z"
                            Fill="{telerik:Office2013Resource ResourceKey=StrongBrush}"
                            HorizontalAlignment="Left"
                            Margin="1,0,0,0"
                            Stretch="Fill"
                            Width="7"
                            Height="9"
                            VerticalAlignment="Bottom"
