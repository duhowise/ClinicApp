                    </Grid.RenderTransform>
                </Grid>
                <Grid x:Name="LineDown" Opacity="0" Background="Transparent" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <Path
                            Data="M4,0L4,7 0,3.35z"
