    <Style TargetType="telerikNavigation:RadBreadcrumb" BasedOn="{StaticResource RadBreadcrumbStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBar" BasedOn="{StaticResource RadBreadcrumbBarStyle}"/>
    <Style TargetType="telerikBreadcrumb:RadBreadcrumbBarItem" BasedOn="{StaticResource RadBreadcrumbBarItemStyle}"/>
    <ControlTemplate x:Key="CarouselDataFieldPresenterTemplate" TargetType="{x:Type telerikNavigation:CarouselDataFieldPresenter}">
